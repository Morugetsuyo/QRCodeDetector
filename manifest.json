{
  "manifest_version": 3,
  "version": "2.0.1",
  "name": "QR Code Detector dev_ver-var OS compatible model",
  "description": "QR Code Detector dev_ver-var OS compatible model",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "optional_permissions": [
    "declarativeNetRequestWithHostAccess",
    "sidePanel"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "data/window/index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["data/window/index.html", "data/window/web_worker.js", "data/window/html2canvas.min.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "data/icons.png",
    "32": "data/icons.png",
    "48": "data/icons.png",
    "128": "data/icons.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}